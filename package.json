{
  "name": "puml2code",
  "version": "0.1.0",
  "description": "PlantUML to code generator",
  "main": "index.js",
  "bin": {
    "puml2code": "npm run cli"
  },
  "scripts": {
    "cli": "node src/cli.js",
    "test": "MOCHA_FILE=junit/test-results.xml istanbul cover -- _mocha --recursive -R mocha-circleci-reporter",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jupe/puml2ode.git"
  },
